          "paint": {
            "fillAntiAlias": true,
            "fillOpacity": 0.2,
            "fillColor": "black",
            "fillOutlineColor": "black"
          }
        },
        {
          "id": "control_branches",
          "visible": true,
          "name": "Drain Network",
          "locationSetId": "control_branches",
